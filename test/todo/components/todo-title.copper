
<todo-title>
	Todo ({{ count }})
</todo-title>

<script type="module">
	import { number, maxValue, safeParse } from 'valibot';

	const { count } = defineProps({
		count: (value) => safeParse(
			number([
				maxValue(10),
			]),
			value,
		).success,
	});
</script>

<style>
	todo-title {
		display: block;
		font-size: 1.5em;
		font-weight: bold;
		margin-bottom: .5em;
	}
</style>
